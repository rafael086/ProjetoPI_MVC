@model List<ProjetoPI_MVC.Models.Sobre>
@foreach (var sobre in Model.Where(s => s.IdImagem != null))
{
<div class="row" data-pessoal="@sobre.Id">
    <div class="col-md-4">
        <img src="~/ImagensUsuarios/@sobre.Imagens.Nome" class="img-circle" alt="">
    </div>
    <div class="cold-md-8 panel panel-default">
        <div class="panel-heading">
            <h3>@sobre.Titulo</h3>
        </div>
        <div class="panel-body">
            <p>@sobre.Texto</p>
            @if (!ViewBag.Visitante)
            {
                <button type="button" class="btn btn-default btnEditar">Editar</button>
            }
        </div>
    </div>
</div>

}
